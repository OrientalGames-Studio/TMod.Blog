@using System.Reflection

<Router AppAssembly="typeof(Program).Assembly" AdditionalAssemblies="[typeof(Core._Imports).Assembly]">
    <Found Context="routeData">
        <RouteView RouteData="routeData" DefaultLayout="typeof(Core.Shared.Layouts.MainLayout)" />
        <FocusOnNavigate RouteData="routeData" Selector="h1" />
    </Found>
</Router>